<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="app.mtproject" android:versionCode="1" android:versionName="1.0">
	<uses-permission android:name="android.permission.READ_CONTACTS" />
	<uses-permission android:name="android.permission.READ_SMS" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-sdk android:minSdkVersion="10" />
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".MtprojectActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".Login" android:label="MtProject">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<service android:name="CallsLoggingService" android:process=":remote">
			<intent-filter>
				<action android:name="app.mtproject.IMyRemoteCallsLoggingService" />
			</intent-filter>
		</service>
		<service android:name="SmsLoggingService" android:process=":remote">
			<intent-filter>
				<action android:name="app.mtproject.IMyRemoteSmsLoggingService" />
			</intent-filter>
		</service>
	</application>
</manifest>